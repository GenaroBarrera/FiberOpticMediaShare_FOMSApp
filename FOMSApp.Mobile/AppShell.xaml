<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="FOMSApp.Mobile.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:views="clr-namespace:FOMSApp.Mobile.Views"
    Title="FOMSApp">

    <ShellContent
        Title="Vaults"
        ContentTemplate="{DataTemplate views:VaultsPage}"
        Route="VaultsPage" />

    <ShellContent
        Title="Midpoints"
        ContentTemplate="{DataTemplate views:MidpointsPage}"
        Route="MidpointsPage" />

    <ShellContent
        Title="Cables"
        ContentTemplate="{DataTemplate views:CablesPage}"
        Route="CablesPage" />

</Shell>
